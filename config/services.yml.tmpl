http:
  services:
    ovpn-starter:
      loadBalancer:
        servers:
          - url: http://${OPENVPN_IP}:${OPENVPN_PORT}
        passHostHeader: true
    nginx-static:
      loadBalancer:
        servers:
          - url: http://${OPENVPN_IP}:80
    noop:
    loadBalancer:
      servers:
        - url: "http://127.0.0.1"